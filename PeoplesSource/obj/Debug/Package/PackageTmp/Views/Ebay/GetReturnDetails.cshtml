@model PeoplesSource.Models.ReturnDetailModel
@{
    Layout = null;
}

@* Style *@
<style>
    .bootstrap-dialog-close-button {
    cursor:pointer;
    }
    .ATags {
    float:left !important;
    }
</style>

@* CSS *@
<link href="~/Content/Theme/css/plugins/iCheck/custom.css" rel="stylesheet" />
<link href="@Url.Content("~/Content/Theme/css/bootstrap.min.css")" rel="stylesheet" type="text/css"/>
<link href="@Url.Content("~/Content/Theme/font-awesome/css/font-awesome.min.css")" rel="stylesheet"  type="text/css" />
<link href="@Url.Content("~/Content/Theme/css/style.css")" rel="stylesheet" type="text/css"/>
<link href="@Url.Content("~/Content/Theme/css/plugins/toastr/toastr.min.css")" rel="stylesheet" />
<link href="~/Content/Theme/css/plugins/TagInput/bootstrap-tagsinput.css" rel="stylesheet" />
<link href="@Url.Content("~/Content/css/bootstrap-dialog.min.css")" rel="stylesheet" type="text/css" />

@* JS *@
<script src="@Url.Content("~/Content/Theme/js/jquery-2.1.1.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/bootstrap.min.js")" type="text/javascript"></script>
<script src="~/Content/Theme/ckeditor/ckeditor.js"></script>
<script src="~/Content/Theme/js/plugins/toastr/toastr.min.js"></script> 
<script src="~/Content/Theme/Page/js/NewPageMessage.js"></script>
<script src="~/Content/Theme/js/TagInput/bootstrap-tagsinput.min.js"></script>
<script src="@Url.Content("~/Scripts/bootstrap-dialog.min.js")" type="text/javascript"></script>

@* Main Design *@
<div class="row">
    <div class="col-lg-12">
          
        @if (Model == null)
        {
            <div>Data Not Available.</div>
        }
        else
        {
            <div class="panel panel-default">
          
                <!-- /.panel-heading -->
                <div id="ActionBtn" style="margin-top:10px;margin-left:15px;margin-bottom:5px;"></div>
                <div style="margin-bottom:5px;margin-left:-5px;" id="DetailTagList" class="col-lg-12"></div>
                <div class="panel-body">
                    <div>
                        <div style="font-size:20px;font-weight:bolder;margin-bottom:10px;">Return Details :</div>
                        <table class="table table-striped table-bordered table-hover footable" data-filter="#filter">
                            <tr>
                                <td>Ack</td>
                                <td>@Model.Ack</td>
                                <td>Time Stamp</td>
                                <td>@Model.TimeStamp</td>
                            </tr>
                            <tr>
                                <td>Version</td>
                                <td>@Model.Version</td>
                                <td>Buyer Return Shipment CarrierUsed</td>
                                <td>@Model.BuyerReturnShipmentCarrierUsed</td>
                            </tr>
                            <tr>
                                <td>Buyer Return Shipment DeliveryDate</td>
                                <td>@Model.BuyerReturnShipmentDeliveryDate</td>
                                <td>Buyer Return Shipment Max Est DeliveryDate</td>
                                <td>@Model.BuyerReturnShipmentMaxEstDeliveryDate</td>
                            </tr>
                            <tr>
                                <td>Buyer Return Shipment Min Est DeliveryDate</td>
                                <td>@Model.BuyerReturnShipmentMinEstDeliveryDate</td>
                                <td>Buyer Return Shipment Merchandise Authorization</td>
                                <td>@Model.BuyerReturnShipmentMerchandiseAuthorization</td>
                            </tr>
                            <tr>
                                <td>Buyer Return Shipment Status</td>
                                <td>@Model.BuyerReturnShipmentStatus</td>
                                <td>Buyer Return Shipment City</td>
                                <td>@Model.BuyerReturnShipmentCity</td>
                            </tr>
                            <tr>
                                <td>Buyer Return Shipment Country</td>
                                <td>@Model.BuyerReturnShipmentCountry</td>
                                <td>Buyer Return Shipment County</td>
                                <td>@Model.BuyerReturnShipmentCounty</td>
                            </tr>
                            <tr>
                                <td>Buyer Return Shipment Name</td>
                                <td>@Model.BuyerReturnShipmentName</td>
                                <td>Buyer Return Shipment Postal Code</td>
                                <td>@Model.BuyerReturnShipmentPostalCode</td>
                            </tr>
                            <tr>
                                <td>Buyer Return Shipment Postal State Or Province</td>
                                <td>@Model.BuyerReturnShipmentPostalStateOrProvince</td>
                                <td>Buyer Return Shipment Street1</td>
                                <td>@Model.BuyerReturnShipmentStreet1</td>
                            </tr>
                            <tr>
                                <td>Buyer Return Shipment Street2</td>
                                <td>@Model.BuyerReturnShipmentStreet2</td>
                                <td>Buyer Return Shipment Cost</td>
                                <td>@Model.BuyerReturnShipmentCost</td>
                            </tr>
                            <tr>
                                <td>Buyer Return Shipment Tracking Number</td>
                                <td>@Model.BuyerReturnShipmentTrackingNumber</td>
                                <td>Case Id</td>
                                <td>@Model.CaseId</td>
                            </tr>
                            <tr>
                                <td>Case type</td>
                                <td>@Model.CaseType</td>
                                <td>Global Id</td>
                                <td>@Model.GlobalId</td>
                            </tr>
                            <tr>
                                <td>Actual Refund Date</td>
                                <td>@Model.ActualRefundDate</td>
                                <td>Actual Refund Status</td>
                                <td>@Model.ActualRefundStatus</td>
                            </tr>
                            <tr>
                                <td>Actual Refund Total Amount</td>
                                <td>@Model.ActualRefundTotalAmount</td>
                                <td>Estimate Refund Total Amount</td>
                                <td>@Model.EstimateRefundTotalAmount</td>
                            </tr>
                            <tr>
                                <td>Refund Due Date</td>
                                <td>@Model.RefundDueDate</td>
                                <td></td>
                                <td></td>
                            </tr>
                        </table>
                    </div>
                    <div>
                        <div style="font-size:20px;font-weight:bolder;margin-bottom:10px;">Return Summary :</div>
                        <table class="table table-striped table-bordered table-hover footable" data-filter="#filter">
                            <tr style="display: none;">
                                <td>
                                    @if (Model.ReturnSummery != null)
                                    {
                                        if (!string.IsNullOrEmpty(Model.ReturnSummery.ReturnRequestComment) || !string.IsNullOrEmpty(Model.ReturnSummery.ReturnRequestReasonCode)
                                            || !string.IsNullOrEmpty(Model.ReturnSummery.ReturnRequestReasonContent) || !string.IsNullOrEmpty(Model.ReturnSummery.ReturnRequestReasonDescription))
                                        {
                                            <ul>
                                                @if (!string.IsNullOrEmpty(Model.ReturnSummery.ReturnRequestReasonCode))
                                                {
                                                    <li>ReasonCode: @Model.ReturnSummery.ReturnRequestReasonCode</li>
                                                }
                                                @if (!string.IsNullOrEmpty(Model.ReturnSummery.ReturnRequestReasonContent))
                                                {
                                                    <li>Request Reason Content: @Model.ReturnSummery.ReturnRequestReasonContent</li>
                                                }
                                                @if (!string.IsNullOrEmpty(Model.ReturnSummery.ReturnRequestComment))
                                                {
                                                    <li>Comment: @Model.ReturnSummery.ReturnRequestComment</li>
                                                }
                                                @if (!string.IsNullOrEmpty(Model.ReturnSummery.ReturnRequestReasonDescription))
                                                {
                                                    <li>Request Reason Description: @Model.ReturnSummery.ReturnRequestReasonDescription</li>
                                                }
                                            </ul>
                                        }
                                        if (Model.ReturnSummery.ReturnRequestItems.Any())
                                        {
                                            <table class="table table-striped table-bordered table-hover footable" data-filter="#filter">
                                                <thead>
                                                    <tr>
                                                        <th>ItemId</th>
                                                        <th>Item Quantity</th>
                                                        <th>TransactionId</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @foreach (var ReturnRequestItem in Model.ReturnSummery.ReturnRequestItems)
                                                    {
                                                        <tr>
                                                            <td>
                                                                @ReturnRequestItem.ReturnRequestItemId
                                                            </td>
                                                            <td>
                                                                @ReturnRequestItem.ReturnRequestItemQuantity
                                                            </td>
                                                            <td>
                                                                @ReturnRequestItem.ReturnRequestTransactionId
                                                            </td>
                                                        </tr>
                                                    }
                                                </tbody>
                                            </table>
                                        }
                                    }

                                </td>
                            </tr>

                        </table>
                    </div>
                    <div>
                        <div style="font-size:20px;font-weight:bolder;margin-bottom:10px;"> Error Items :</div>
                        <table class="table table-striped table-bordered table-hover footable" data-filter="#filter">
                            <thead>
                                <tr>
                                    <th>Error Id</th>
                                    <th>Error Category</th>
                                    <th>Error Domain</th>
                                    <th>Error ExceptionId</th>
                                    <th>Error Parameters</th>
                                    <th>Message</th>
                                    <th>Severity</th>
                                    <th>Subdomain</th>
                                </tr>
                            </thead>
                            <tbody>
                                @if (Model.ErrorItems.Any())
                                {
                                    foreach (var item in Model.ErrorItems)
                                    {
                                        <tr>
                                            <td>
                                                @item.ErrorId
                                            </td>
                                            <td>
                                                @item.ErrorCategory
                                            </td>
                                            <td>
                                                @item.ErrorDomain
                                            </td>
                                            <td>
                                                @item.ErrorExceptionId
                                            </td>
                                            <td>
                                                @item.ErrorParameters
                                            </td>
                                            <td>
                                                @item.Message
                                            </td>
                                            <td>
                                                @item.Severity
                                            </td>
                                            <td>
                                                @item.Subdomain
                                            </td>
                                        </tr>
                                        <tr style="display: none;">
                                            <td colspan="8">

                                                @if (item.ErrorParameters.Any())
                                                {
                                                    <table class="table table-striped table-bordered table-hover footable" data-filter="#filter">
                                                        <thead>
                                                            <tr>
                                                                <th>Name</th>
                                                                <th>Value</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            @foreach (var itemError in item.ErrorParameters)
                                                            {
                                                                <tr>
                                                                    <td>
                                                                        @itemError.Name
                                                                    </td>
                                                                    <td>
                                                                        @itemError.Value
                                                                    </td>
                                                                </tr>
                                                            }
                                                        </tbody>
                                                    </table>
                                                }
                                            </td>
                                        </tr>
                                    }
                                }
                                else
                                {
                                    <tr>
                                        <td colspan="8">
                                            <label>No Record Found.</label>
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                    <div>
                        <div style="font-size:20px;font-weight:bolder;margin-bottom:10px;">Shipping Information :</div>
                        <table class="table table-striped table-bordered table-hover footable" data-filter="#filter">
                            <thead>
                                <tr>
                                    <th>Carrier Used</th>
                                    <th>Name</th>
                                    <th>Currency Id</th>
                                    <th>Delivery Date</th>
                                    <th>Shipment Status</th>
                                    <th>City</th>
                                    <th>Country</th>
                                    <th>Shipping Cost</th>
                                </tr>
                            </thead>
                            <tbody>
                                @if (Model.ShippingInfo.Any())
                                {
                                    foreach (var item in Model.ShippingInfo)
                                    {
                                        <tr>
                                            <td>
                                                @item.CarrierUsed
                                            </td>
                                            <td>
                                                @item.Name
                                            </td>
                                            <td>
                                                @item.CurrencyId
                                            </td>
                                            <td>
                                                @item.DeliveryDate
                                            </td>
                                            <td>
                                                @item.ShipmentStatus
                                            </td>
                                            <td>
                                                @item.City
                                            </td>
                                            <td>
                                                @item.Country
                                            </td>
                                            <td>
                                                @item.ShippingCost
                                            </td>
                                        </tr>
                                    }
                                }
                                else
                                {
                                    <tr>
                                        <td colspan="16">
                                            <label>No Record Found.</label>
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                    <div>
                        <div style="font-size:20px;font-weight:bolder;margin-bottom:10px;">Return History :</div>
                        <table class="table table-striped table-bordered table-hover footable" data-filter="#filter">
                            <thead>
                                <tr>
                                    <th>Activity Code</th>
                                    <th>Activity Content</th>
                                    <th>Activity Description</th>
                                    <th>Author Role</th>
                                    <th>Author UserId</th>
                                    <th>Creation Date</th>
                                    <th>Note</th>
                                </tr>
                            </thead>
                            <tbody>
                                @if (Model.ReturnHistorys.Any())
                                {
                                    foreach (var item in Model.ReturnHistorys)
                                    {
                                        <tr>
                                            <td>
                                                @item.ActivityCode
                                            </td>
                                            <td>
                                                @item.ActivityContent
                                            </td>
                                            <td>
                                                @item.ActivityDescription
                                            </td>
                                            <td>
                                                @item.AuthorRole
                                            </td>
                                            <td>
                                                @item.AuthorUserId
                                            </td>
                                            <td>
                                                @item.CreationDate
                                            </td>
                                            <td>
                                                @item.Note
                                            </td>
                                        </tr>
                                    }
                                }
                                else
                                {
                                    <tr>
                                        <td colspan="7">
                                            <label>No Record Found.</label>
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                    <div>
                        <div style="font-size:20px;font-weight:bolder;margin-bottom:10px;">Actual Itemized Refund :</div>
                        <table class="table table-striped table-bordered table-hover footable" data-filter="#filter">
                            <thead>
                                <tr>
                                    <th>Amount</th>
                                    <th>Currency Id</th>
                                    <th>Fee Type</th>
                                </tr>
                            </thead>
                            <tbody>
                                @if (Model.ActualItemizedRefund.Any())
                                {
                                    foreach (var item in Model.ActualItemizedRefund)
                                    {
                                        <tr>
                                            <td>
                                                @item.Amount
                                            </td>
                                            <td>
                                                @item.CurrencyId
                                            </td>
                                            <td>
                                                @item.FeeType
                                            </td>

                                        </tr>
                                    }
                                }
                                else
                                {
                                    <tr>
                                        <td colspan="3">
                                            <label>No Record Found.</label>
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                    <div>
                        <div style="font-size:20px;font-weight:bolder;margin-bottom:10px;">Estimate Itemized Refund :</div>
                        <table class="table table-striped table-bordered table-hover footable" data-filter="#filter">
                            <thead>
                                <tr>
                                    <th>Amount</th>
                                    <th>Currency Id</th>
                                    <th>Fee Type</th>
                                </tr>
                            </thead>
                            <tbody>
                                @if (Model.EstimateItemizedRefund.Any())
                                {
                                    foreach (var item in Model.EstimateItemizedRefund)
                                    {
                                        <tr>
                                            <td>
                                                @item.Amount
                                            </td>
                                            <td>
                                                @item.CurrencyId
                                            </td>
                                            <td>
                                                @item.FeeType
                                            </td>

                                        </tr>
                                    }
                                }
                                else
                                {
                                    <tr>
                                        <td colspan="3">
                                            <label>No Record Found.</label>
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                    <div>
                        <div style="font-size:20px;font-weight:bolder;margin-bottom:10px;">Estimate Itemized Optional refund :</div>
                        <table class="table table-striped table-bordered table-hover footable" data-filter="#filter">
                            <thead>
                                <tr>
                                    <th>Amount</th>
                                    <th>Currency Id</th>
                                    <th>Fee Type</th>
                                </tr>
                            </thead>
                            <tbody>
                                @if (Model.EstimateItemizedOptionalRefund.Any())
                                {
                                    foreach (var item in Model.EstimateItemizedOptionalRefund)
                                    {
                                        <tr>
                                            <td>
                                                @item.Amount
                                            </td>
                                            <td>
                                                @item.CurrencyId
                                            </td>
                                            <td>
                                                @item.FeeType
                                            </td>

                                        </tr>
                                    }
                                }
                                else
                                {
                                    <tr>
                                        <td colspan="3">
                                            <label>No Record Found.</label>
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        }
        <!-- /.panel-body -->
    </div>
    <!-- /.panel -->
    <!-- /.col-lg-12 -->
</div>

@*Popup Note*@
<div aria-hidden="true" role="dialog" tabindex="-1" id="Notes_Popup" class="modal inmodal fade" style="display: none;">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header bootstrap-dialog-draggable" style="background-color: #1ab394; border-top-left-radius: 4px; border-top-right-radius: 4px; border-bottom: 1px solid #e5e5e5; min-height: 16.43px; padding: 15px;">
                <div class="bootstrap-dialog-header">
                    <div style="display: block; float: right; opacity: 0.9;" class="bootstrap-dialog-close-button" onclick="ClosePopup();return false;"><i class="fa fa-times" style="color: #FFF"></i></div>
                    <div class="bootstrap-dialog-title" style="font-size: 16px; color: #ffffff; display: inline-block;">Add Note</div>
                </div>
            </div>
            <div class="modal-body">
                <div class="bootstrap-dialog-body">
                    <div class="bootstrap-dialog-message">
                        <div id="modelWindow">
                            <form novalidate="novalidate" id="AddNewNote" method="post">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="form-horizontal">
                                            <div class="form-group">
                                                <div class="col-md-12">
                                                    <textarea cols="40" style="min-height: 135px;" data-val="true" class="form-control" id="NoteContent" name="NoteContent" rows="2" placeholder="Write Note"></textarea>
                                                </div>
                                                <span class="field-validation-valid" data-valmsg-for="TemplateContent" data-valmsg-replace="true"></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <div class="bootstrap-dialog-footer">
                    <div class="bootstrap-dialog-footer-buttons">
                        <button id="Save_Notes" class="btn btn-primary">Save</button>
                        <button class="btn btn-default" onclick="ClosePopup();return false;">Cancel</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@*Assign Tags*@
<div aria-hidden="true" role="dialog" tabindex="-1" id="Tag_Popup" class="modal inmodal fade" style="display: none;">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header bootstrap-dialog-draggable" style="background-color: #1ab394; border-top-left-radius: 4px; border-top-right-radius: 4px; border-bottom: 1px solid #e5e5e5; min-height: 16.43px; padding: 15px;">
                <div class="bootstrap-dialog-header">
                    <div style="display: block; float: right; opacity: 0.9;" class="bootstrap-dialog-close-button" onclick="ClosePopup();return false;"><i class="fa fa-times" style="color: #FFF"></i></div>
                    <div class="bootstrap-dialog-title" style="font-size: 16px; color: #ffffff; display: inline-block;">Add Tag</div>
                </div>
            </div>
            <div class="modal-body">
                <div class="bootstrap-dialog-body">
                    <div class="bootstrap-dialog-message">
                        <div id="modelWindow">
                            <form novalidate="novalidate" id="AddNewTags" method="post">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="form-horizontal">
                                            <div class="form-group">
                                                <div class="col-md-2">
                                                    <span style="font-size:17px;font-weight:bolder;" class="pull-right">Tag : </span>
                                                </div>
                                                <div class="col-md-10">
                                                    <input type="text"  value="" name="Tags" id="Txt_Tags" data-val-required="Tags Required" data-val="true" class="form-control" autofocus>
                                                </div>
                                                <span class="field-validation-valid" data-valmsg-for="Tags" data-valmsg-replace="true"></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <div class="bootstrap-dialog-footer">
                    <div class="bootstrap-dialog-footer-buttons">
                        <button id="Save_Tag" class="btn btn-primary">Save</button>
                        <button class="btn btn-default" onclick="ClosePopup();return false;">Cancel</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@*Popup Form*@
<div style="display: none;" class="modal inmodal" id="AssignTag_PopUp" tabindex="-1" role="dialog" aria-hidden="false">
    <div class="modal-dialog" style="width: 350px;">
        <div class="modal-content animated bounceInRight">
            <div class="modal-header" style="padding: 4px 5px;">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>

                <h4 class="modal-title">Tags</h4>
                <small class="font-bold">You can select multiple tags to assign.</small>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-lg-12">
                        <div id="AssignTagList" style="text-align: center;min-height:380px;max-height:380px;overflow:auto;">
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" id="Savebtn" onclick="SaveAssignTag();return false;">Save</button>
                <button type="button" class="btn btn-white" data-dismiss="modal">Close</button>

            </div>
        </div>
    </div>
</div>



