        </nav>

        <div id="page-wrapper" class="gray-bg">
            <div class="row border-bottom">
                <nav class="navbar navbar-static-top  " role="navigation" style="margin-bottom: 0">
                    <div class="navbar-header">
                        <a class="navbar-minimalize minimalize-styl-2 btn btn-primary " onclick="Minimize();return false;" href="#"><i class="fa fa-bars"></i></a>

                    </div>
                    <ul class="nav navbar-top-links navbar-right">
                        <li>
                            <span class="m-r-sm text-muted welcome-message">Welcome to SyncTu</span>
                        </li>

                        <li>
                            <a href="#" onclick="logOut();return false;">
                                <label class="LogoutButton" style="color:#1ab394;font-weight:bold;"><i class="fa fa-sign-out"></i>Log out</label>
                            </a>
                        </li>
                    </ul>

                </nav>
            </div>
            <div class="row wrapper border-bottom white-bg page-heading">
                <div class="col-sm-12">
                    <h2>@ViewBag.title</h2>
                </div>
            </div>
            @RenderBody()


        </div>

    </div>
    <div>
        <div style="color:#CCC;text-align:center">
            <span>Copyright</span> SyncTu &copy;
        </div>
    </div>


    <script type="text/javascript">

        $(window).resize(function () {
            if ($(this).width() < 769) {
                $('body').addClass('body-small')
            } else {
                $('body').removeClass('body-small')
            }
        });
        function logOut() {
            var url = '@Url.Action("Logout", "Account")';
            window.localStorage.clear();
            NavigateTo(url);

        }



        function isNumberKey(evt) {
            var charCode = (evt.which) ? evt.which : evt.keyCode;
            if (charCode > 31 && (charCode < 48 || charCode > 57) && charCode != 46 && charCode != 35 && charCode != 36 && charCode != 37 && charCode != 39)
                return false;

            return true;
        }

        function isNumberWithSingleDash(evt) {
            var charCode = (evt.which) ? evt.which : evt.keyCode;
            if (charCode > 31 && (charCode < 48 || charCode > 57) && charCode != 46 && charCode != 45 && charCode != 35 && charCode != 36 && charCode != 37 && charCode != 39)
                return false;

            return true;
        }

        function onlyDecimal(evt) {
            var charCode = (evt.which) ? evt.which : evt.keyCode;
            if (charCode != 46 && charCode > 31 && (charCode < 48 || charCode > 57) && charCode != 35 && charCode != 36 && charCode != 37 && charCode != 39)
                return false;

            return true;
        }
    </script>

</body>
</html>
